#include <iostream>
#include <fstream>
#include <cmath>
#define _USE_MATH_DEFINES
#include <math.h>
using namespace std;

int main() {

    double Init, Inc, Final, Volume;

    cout << "Input your Inital Radius, Incrament, and Final Radius seperated by one space";

    cin >> Init >> Inc >> Final;

    while (Init + Inc <= Final or Init == Final)
    {
        Volume = M_PI * 4
    }



    return 0;
}
